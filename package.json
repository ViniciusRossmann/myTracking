{
  "name": "rastrear_server",
  "version": "1.0.0",
  "description": "Servidor do aplicativo de rastreio.",
  "main": "index.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "npm run start",
    "mobile": "cd mobile && expo start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" \"npm run mobile\"",
    "start": "ts-node-dev server/src/server.ts",
    "server-client": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
